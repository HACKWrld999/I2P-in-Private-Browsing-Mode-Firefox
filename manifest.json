{
  "manifest_version": 2,
  "name": "I2P Proxy",
  "version": "2.0",
  "description": "Set up a browser to use the i2p http proxy automatically",
  "background": {
    "scripts": ["background.js", "proxy.js"]
  },
  "permissions": ["theme", "proxy"],
  "applications": {
    "gecko": {
      "id": "i2psetproxy.js@eyedeekay.github.io",
      "strict_min_version": "60.0"
    }
  }
}
